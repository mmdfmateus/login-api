  },
  "homepage": "https://github.com/mmdfmateus/login-api#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.23",
    "supertest": "^4.0.2",
    "validator": "^13.1.1"
